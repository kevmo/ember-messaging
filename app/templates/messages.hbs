{{outlet}}

<div class="ui feed">
{{#each model as |message|}}
    <div class=event>
        <div class="label"><img src="{{ message.pic }}" /></div>
        <div class="content">
            <span class="summary">
                {{ message.message }} <br>
                {{ message.useragent }}
            </span>
            <span class="meta">
                {{ message.date }}
            </span>
        </div>
    </div>
{{/each}}
</div>
